{
  "name": "@davidingplus/vitepress-waline-live2d",
  "version": "1.1.1",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "description": "vitepress plugin for website",
  "repository": {
    "type": "git",
    "url": "https://github.com/DavidingPlus/vitepress-waline-live2d"
  },
  "keywords": [
    "vitepress",
    "theme",
    "plugin",
    "waline",
    "live2d"
  ],
  "bugs": {
    "url": "https://github.com/DavidingPlus/vitepress-waline-live2d/issues"
  },
  "homepage": "https://github.com/DavidingPlus/vitepress-waline-live2d#readme",
  "author": "DavidingPlus",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/waline-live2d.cjs.js",
  "module": "dist/waline-live2d.es.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/waline-live2d.es.js",
      "require": "./dist/waline-live2d.cjs.js"
    },
    "./waline": {
      "types": "./dist/types/waline/components/Waline.d.ts",
      "import": "./dist/waline-live2d.es.js",
      "require": "./dist/waline-live2d.cjs.js"
    },
    "./live2d": {
      "types": "./dist/types/live2d/components/Live2dComponent.vue.d.ts",
      "import": "./dist/waline-live2d.es.js",
      "require": "./dist/waline-live2d.cjs.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "stub": "unbuild --stub",
    "test": "jest",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx,.vue",
    "release": "yunque-release",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "@waline/client": "^3.1.3"
  },
  "devDependencies": {
    "@types/node": "^20.12.3",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@yunquejs/release": "^0.1.9",
    "conventional-changelog-cli": "^4.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.24.0",
    "jest": "^30.1.3",
    "lint-staged": "^15.2.2",
    "picocolors": "^1.0.0",
    "prettier": "^3.2.5",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "unbuild": "^2.0.0",
    "vite": "^7.1.5",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "^2.0.0-alpha.6",
    "vitest": "^1.4.0",
    "vue": "^3.4.21",
    "vue-eslint-parser": "^9.4.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "pnpm exec tsx scripts/verifyCommit.ts $1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}