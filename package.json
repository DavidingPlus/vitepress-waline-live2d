{
  "name": "vitepress-theme-website",
  "version": "1.0.0",
  "type": "module",
  "description": "vitepress plugin for website",
  "repository": {
    "type": "git",
    "url": "https://github.com/xinlei3166/vitepress-theme-website"
  },
  "keywords": [
    "vitepress",
    "waline",
    "live2d",
    "plugin"
  ],
  "bugs": {
    "url": "https://github.com/xinlei3166/vitepress-theme-website/issues"
  },
  "homepage": "https://github.com/xinlei3166/vitepress-theme-website#readme",
  "author": "君惜",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./waline": {
      "types": "./dist/waline/index.d.ts",
      "import": "./dist/waline/index.mjs"
    },
    "./live2d": {
      "types": "./dist/live2d/index.d.ts",
      "import": "./dist/live2d/index.mjs"
    }
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "test": "jest",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx,.vue",
    "release": "yunque-release --skipBuild",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "dependencies": {
    "vue": "^3.3.2",
    "vitepress": "^1.0.0-alpha.75",
    "@waline/client": "2.15.5-alpha.0"
  },
  "devDependencies": {
    "@types/node": "^20.1.5",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@yunquejs/release": "^0.1.9",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.13.0",
    "lint-staged": "^13.2.2",
    "picocolors": "latest",
    "prettier": "^2.8.8",
    "simple-git-hooks": "latest",
    "tsup": "^6.7.0",
    "tsx": "latest",
    "typescript": "^4.9.5",
    "unbuild": "latest",
    "vitest": "latest",
    "vue-eslint-parser": "^9.3.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "pnpm exec tsx scripts/verifyCommit.ts $1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  }
}
